    "helmet": "^3.6.1",
    "hpp": "^0.2.2",
    "morgan": "^1.8.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "reason-react": "https://github.com/reasonml/reason-react.git",
    "sanitize.css": "^5.0.0"
  },
  "devDependencies": {
    "bs-platform": "^1.7.3",
    "razzle": "^0.6.3"
  }
}