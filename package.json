{
  "name": "reasonmlnyc-web",
  "version": "0.6.3",
  "license": "MIT",
  "scripts": {
    "dev": "npm run js-watch | npm run bsb-watch",
    "test": "jest",
    "js-watch": "razzle start",
    "bsb-watch": "bsb -make-world -w 2>&1 | berror --path-to-refmttype  /usr/local/bin/refmt",
    "clean": "bsb -clean-world && rm -rf build",
    "build": "bsb -make-world && razzle build",
    "start": "NODE_ENV=production node build/server.js"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "bs-director": "^0.0.1",
    "compression": "^1.6.2",
    "express": "^4.15.2",
    "helmet": "^3.6.1",
    "hpp": "^0.2.2",
    "lru-cache": "^4.0.2",
    "morgan": "^1.8.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router-dom": "^4.1.1",
    "reason-react": "0.1.4",
    "sanitize.css": "^5.0.0",
    "serialize-javascript": "^1.3.0"
  },
  "devDependencies": {
    "bs-jest": "^0.1.0",
    "bs-platform": "https://github.com/bloomberg/bucklescript",
    "bs-react-test-renderer": "^1.0.0",
    "jest": "^20.0.4",
    "ocamlBetterErrors": "^0.1.1",
    "razzle": "^0.6.3",
    "sw-precache-webpack-plugin": "^0.9.2"
  },
  "jest": {
    "roots": [
      "src"
    ]
  }
}
